DESCRIPTION="Implements a XML/HTML/XHTML Markup safe string for Python"
HOMEPAGE="http://pypi.python.org/pypi/MarkupSafe"
LICENSE="custom"
PACKAGER="StartOS Developers"
YARCH="any"

SRC_URI="http://pypi.python.org/packages/source/M/MarkupSafe/MarkupSafe-"$V".tar.gz"

RDEPEND="python"
BDEPEND="distribute"
RECOMMENDED=""

pbs_unpack() {
        dounpack
}

pbs_build() {
	domake
}

pbs_install() {
	domkinstall --optimize=1
	dodoc LICENSE
}



