DESCRIPTION="CDDB-Server access from Python"
HOMEPAGE="http://pycddb.sourceforge.net"
LICENSE="GPL"
PACKAGER="StartOS Developers"
ARCH="any"

SRC_URI="http://downloads.sourceforge.net/pycddb/PyCDDB-"$V".tar.gz"

