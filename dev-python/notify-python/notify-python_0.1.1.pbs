#
# YLmf_OS package build script
#

DESCRIPTION="Python bindings for libnotify"
HOMEPAGE="http://www.galago-project.org"
LICENSE="GPL-2"
PACKAGER="hzx@115.com"

SRC_URI="http://www.galago-project.org/files/releases/source/"$N"/"$N-$V$R".tar.gz"

RDEPEND=""
BDEPEND="${RDEPEND} libnotify"
RECOMMENDED=""

NOTES=""

pbs_unpack() {
        ypkg_unpack
}

pbs_config() {
	# Remove the old pynotify.c to ensure it's properly regenerated #212128.
	rm -f src/pynotify.c
	ypkg_config
}

pbs_build() {
	ypkg_make
}

pbs_install() {
	ypkg_mkinstall
}
