#
# YLmf_OS package build script
#

DESCRIPTION="A fork from google pinyin on android ibus wrapper"
HOMEPAGE="http://code.google.com/p/libgooglepinyin"
LICENSE="Apache License 2.0"
PACKAGER="Ylmf OS Developers <ylmfos@115.com>"

SRC_URI="http://libgooglepinyin.googlecode.com/files/"$N-$V$R".tar.bz2"

RDEPEND="libgooglepinyin"
RECOMMENDED=""
BDEPEND=""
CONFLICT=""

pbs_unpack() {
        ypkg_unpack
}

pbs_config() {
	YPB_CONFIG+=""
	ypkg_config
}

pbs_build() {
	ypkg_make
}

pbs_install() {
	ypkg_mkinstall
}

