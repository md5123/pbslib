#
# YLmf_OS package build script
#

DESCRIPTION="ALSA extra plugins"
HOMEPAGE="http://www.alsa-project.org/"
LICENSE="GPL"
PACKAGER="Ylmf OS Developers <ylmfos@115.com>"

SRC_URI="ftp://ftp.alsa-project.org/pub/plugins/"$N-$V".tar.bz2"

RDEPEND="alsa-lib ffmpeg glibc jack-audio-connection-kit libsamplerate libva zlib"
BDEPEND=""

pbs_unpack() {
        ypkg_unpack
}

pbs_config() {
	ypkg_config
}

pbs_build() {
	ypkg_make
}

pbs_install() {
	ypkg_mkinstall
}

