#
# Ylmf OS package build script
#

DESCRIPTION="gnome2 desktop themes for Ylmf OS"
HOMEPAGE="www.ylmf.com"
LICENSE="GPL"
PACKAGER="Ylmf OS Developers <ylmfos@ylmf.com>"
YARCH="any"

SRC_URI="git://git.ylmf.com/ylmfos-themes.git"

RDEPEND="murrine"
BDEPEND=""
RECOMMENDED=""

INSTALL="$N.install"

pbs_unpack() {
	dounpack
}

pbs_install() {
	docp ylmfos "$destdir"/usr/share/themes
}

