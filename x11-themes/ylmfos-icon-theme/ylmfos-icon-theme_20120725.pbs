DESCRIPTION="StartOS default icon theme"
HOMEPAGE="http://www.startos.com"
LICENSE="GPL"
PACKAGER="StartOS Developers"
YARCH="any"

SRC_URI="git://git.ylmf.com/"$N".git"

RDEPEND=""
BDEPEND=""
RECOMMENDED="startos-icon-theme"
CONFLICT="startos-icon-theme"

INSTALL="$N.install"

pbs_unpack() {
	dounpack
}

pbs_install() {
	docp startos "$destdir"/usr/share/icons
}

