#
# Ylmf OS package build script
#

DESCRIPTION="Ylmfos default icon theme"
HOMEPAGE="www.ylmf.com"
LICENSE="GPL"
PACKAGER="Ylmf OS Developers <ylmfos@ylmf.com>"
YARCH="any"

SRC_URI="git://git.ylmf.com/"$N".git"

RDEPEND=""
BDEPEND=""

INSTALL="${N}2.install"

pbs_unpack() {
	dounpack
}

pbs_install() {
	docp ylmfos "$destdir"/usr/share/icons
}

