DESCRIPTION="SVG and PNG icon theme from the Tango project"
HOMEPAGE="http://tango.freedesktop.org"
 
SRC_URI="http://tango.freedesktop.org/releases/"$N-$V$R".tar.gz"

RDEPEND=""
BDEPEND="imagemagick"

INSTALL="$N.install"

pbs_unpack() {
        dounpack
}

pbs_config() {
    doconfig
}

pbs_build() {
    domake
}

pbs_install() {
    domkinstall
}
