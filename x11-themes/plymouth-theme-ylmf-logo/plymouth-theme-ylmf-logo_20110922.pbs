DESCRIPTION="ylmfos default theme for plymouth"
HOMEPAGE="www.ylmf.net"
LICENSE="GPLv2"
YARCH="any"
PACKAGER="Ylmf OS Developers <ylmfos@115.com>"

SRC_URI="git://git.ylmf.com/"$N".git"

RDEPEND="plymouth"
BDEPEND=""
