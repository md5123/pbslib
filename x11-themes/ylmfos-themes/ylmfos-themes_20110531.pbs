#
# Ylmf OS package build script
#

DESCRIPTION="ylmfos default desktop theme"
HOMEPAGE="www.ylmf.com"
LICENSE="GPL"
PACKAGER="Ylmf OS Developers <ylmfos@115.com>"
YARCH="any"

SRC_URI="git://git.ylmf.com/"$N".git"

RDEPEND="murrine"
BDEPEND=""
RECOMMENDED=""

INSTALL="$N.install"

pbs_unpack() {
	dounpack
}

pbs_install() {
	docp ylmfos "$destdir"/usr/share/themes
}
