DESCRIPTION="Multiple GNOME terminals in one window"
HOMEPAGE="http://www.tenshu.net/terminator/"
LICENSE="GPL"
YARCH="any"
PACKAGER="StartOS Developers"

SRC_URI="http://launchpadlibrarian.net/54318913/"$N-$V".tar.gz"

RDEPEND="python"
BDEPEND=""
RECOMMENDED="keybinder"
CONFLICT=""

INSTALL="$N.install"

pbs_unpack() {
        dounpack
}

pbs_config() {
	config+=""
	doconfig
}

pbs_build() {
	domake
}

pbs_install() {
	domkinstall
}

