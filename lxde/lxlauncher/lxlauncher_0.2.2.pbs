DESCRIPTION="An open source clone of the Asus launcher for EeePC"
HOMEPAGE="http://lxde.org"
LICENSE="GPL"
PACKAGER="StartOS Developers"

SRC_URI="http://sourceforge.net/projects/lxde/files/LXLauncher%20%28for%20Asus%20EeePC%29/LXLauncher%20"$V"/"$N-$V".tar.gz"

RDEPEND="gtk+ menu-cache startup-notification"
BDEPEND=""
RECOMMENDED=""
CONFLICT=""

INSTLL="$N.install"

pbs_unpack() {
        dounpack
}

pbs_config() {
	config+=""
	doconfig
}

pbs_build() {
	domake
}

pbs_install() {
	domkinstall
}

