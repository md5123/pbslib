. /usr/lib/ybs/funcs

files="/etc/gconf/schemas/vinagre.schemas"

post_install() {
	gnome2_install_schema "$files"
	gnome2_desktop_database_update
	gnome2_icon_cache_update 
}

pre_remove() {
	gnome2_uninstall_schema "$files" ||true
}

post_remove() {
	gnome2_desktop_database_update
	gnome2_icon_cache_update
}

post_upgrade() {
	post_install
}

post_downgrade() {
	post_install
}
