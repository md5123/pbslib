#
# YLmf_OS package build script
#

DESCRIPTION="Locale and ISO 2022 support for Unicode terminals"
HOMEPAGE="http://xorg.freedesktop.org/"
 
SRC_URI="http://www.x.org/releases/X11R7.6/src/app/"$N-$V$R".tar.bz2"

RDEPEND="glibc libfontenc zlib"
BDEPEND="glibc-dev libfontenc-dev zlib-dev"

pbs_unpack() {
        ypkg_unpack
}

pbs_config() {
	
	ypkg_config
}

pbs_build() {
	ypkg_make
}

pbs_install() {
	ypkg_mkinstall
}
