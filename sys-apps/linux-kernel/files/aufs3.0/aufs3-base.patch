diff -ur linux-kernel-3.0.20-ylmf1/fs/namei.c linux-kernel-3.0.20-ylmf1.new//fs/namei.c
--- linux-kernel-3.0.20-ylmf1/fs/namei.c	2012-02-07 01:31:45.000000000 +0800
+++ linux-kernel-3.0.20-ylmf1.new//fs/namei.c	2012-04-25 13:54:30.605457216 +0800
@@ -1705,7 +1705,7 @@
  * needs parent already locked. Doesn't follow mounts.
  * SMP-safe.
  */
-static struct dentry *lookup_hash(struct nameidata *nd)
+struct dentry *lookup_hash(struct nameidata *nd)
 {
 	return __lookup_hash(&nd->last, nd->path.dentry, nd);
 }
