#
# Ylmf OS package build script
#

DESCRIPTION="A C library for the arithmetic of complex numbers with arbitrarily high precision and correct rounding of the result."
HOMEPAGE="http://www.multiprecision.org/"
REPO=""
LICENSE="GPL"
PACKAGER="Ylmf OS Developers <ylmfos@ylmf.com>"

SRC_URI="http://www.multiprecision.org/"$N"/download/"$N-$V$R".tar.gz"

RDEPEND="glibc gmp mpfr"
BDEPEND="  "
RECOMMENDED=""

INSTALL="$N.install"

pbs_unpack() {
        dounpack
}

pbs_config() {
	doconfig
}

pbs_build() {
	domake
}

pbs_install() {
	domkinstall
}

