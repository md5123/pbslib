. /usr/lib/ybs/funcs

files="/etc/gconf/schemas/playonlinux-handler.schemas"

post_install() {
	gnome2_install_schema "$files"
	gnome2_desktop_database_update
}

pre_remove() {
	gnome2_uninstall_schema "$files"
}

post_remove() {
	gnome2_desktop_database_update
}

post_upgrade() {
	post_install
}

post_downgrade() {
	post_install
}

