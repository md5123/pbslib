. /usr/lib/ybs/funcs

gname="usbmux"

post_install() {
    dogroupadd -g 140 $gname &>/dev/null
    douseradd -c 'usbmux user' -u 140 -g $gname -d / -s /sbin/nologin $gname &>/dev/null
    passwd -l $gname &>/dev/null
}

post_remove() {
	douserdel -f $gname
    dogroupdel $gname || true
}

post_upgrade() {
	post_install
}

