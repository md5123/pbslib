#
# YLmf_OS package build script
#

DESCRIPTION="Low-level pixel manipulation routines"
HOMEPAGE="http://xorg.freedesktop.org/"
LICENSE="GPL"
PACKAGER="Ylmf OS Developers <ylmfos@115.com>"

SRC_URI="http://cairographics.org/releases/"$N-$V".tar.gz"

RDEPEND="glibc"
BDEPEND=""

pbs_unpack() {
        dounpack
}

pbs_config() {
	config+="--disable-vmx 
	             --disable-gtk 
		     --disable-mmx 
		     --disable-sse2"
	doconfig
}

pbs_build() {
	domake
}

pbs_install() {
	domkinstall
}

