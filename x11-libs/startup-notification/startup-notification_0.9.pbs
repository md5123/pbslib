#
# YLmf_OS package build script
#

DESCRIPTION="Application startup notification and feedback library"
HOMEPAGE="http://www.freedesktop.org/wiki/Software/startup-notification"
 
SRC_URI="http://www.freedesktop.org/software/"$N"/releases/"$N-$V$R".tar.gz"

RDEPEND="libSM libICE libX11 util-linux-ng libxcb libXau libXdmcp"
BDEPEND="${RDEPEND}"

pbs_unpack() {
        ypkg_unpack
}

pbs_config() {
	ypkg_config
}

pbs_build() {
	ypkg_make
}

pbs_install() {
	ypkg_mkinstall
}
