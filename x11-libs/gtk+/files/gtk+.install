. /usr/lib/ybs/funcs

post_install() {
        gnome2_gdk_pixbuf_loaders_update
        gnome2_gtk_immodules_update
}

pre_remove() {
	rm -f /etc/gtk-2.0/gtk.immodules &>/dev/null
	rm -f /etc/gtk-2.0/gdk-pixbuf.loaders &>/dev/null ||true
}

post_upgrade() {
	post_install
}

post_downgrade() {
	post_install
}

