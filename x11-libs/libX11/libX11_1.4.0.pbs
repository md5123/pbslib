#
# YLmf_OS package build script
#

DESCRIPTION="X.Org X11 library"
HOMEPAGE="http://www.x.org/wiki/"
 
SRC_URI="http://www.x.org/releases/X11R7.6/src/lib/"$N-$V$R".tar.bz2"

RDEPEND="glibc libXau libxcb libXdmcp"
BDEPEND="xtrans-dev glibc-dev libXau-dev libxcb-dev libXdmcp-dev"

pbs_unpack() {
        ypkg_unpack
}

pbs_config() {
	#YPB_CONFIG+=""
	ypkg_config
}

pbs_build() {
	ypkg_make
}

pbs_install() {	
	ypkg_mkinstall
}
