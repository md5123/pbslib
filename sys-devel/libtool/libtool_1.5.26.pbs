#
# YLmf_OS package build script
#

DESCRIPTION="A shared library tool for developers"
HOMEPAGE="http://www.gnu.org/software/libtool/"
 
SRC_URI="http://ftp.gnu.org/gnu/libtool/$N-$V.tar.gz"

RDEPEND=""
BDEPEND="${RDEPEND}"

pbs_unpack()
{
        ypkg_unpack
}

pbs_config()
{
	cd $N-$V
	ypkg_config
}

pbs_build()
{
	ypkg_make
}

pbs_install()
{
	ypkg_mkinstall
}
