. /usr/lib/ybs/funcs

files="/etc/gconf/schemas/gucharmap.schemas"

post_install() {
    glib_compile_schemas "$@"
    gtk-update-icon-cache -q -t -f /usr/share/icons/hicolor
}

pre_remove() {
    post_install "$@"
}

post_remove() {
    post_install "$@"
}

post_upgrade() {
    post_install "$@"
}

post_downgrade() {
    post_install "$@"
}

