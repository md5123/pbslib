LICENSE="GPL"
PACKAGER="Ylmf OS Developers <ylmfos@ylmf.com>"

SRC_URI="http://nchc.dl.sourceforge.net/project/lxde/"$N"/"$N"%200.3.2/"$N-$V".tar.gz"

