DESCRIPTION="An API documentation browser for GNOME 2"
HOMEPAGE="http://live.gnome.org/devhelp"
LICENSE="GPL"
PACKAGER="StartOS Developers"

RDEPEND="libunique libwnck webkitgtk"
RECOMMENDED=""
